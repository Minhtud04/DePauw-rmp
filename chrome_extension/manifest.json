{
  "manifest_version": 2,
  "name": "DePauw SOC Edge Alert",
  "version": "1.0",
  "description": "Displays 'Hi' in any webpage",
  "content_scripts": [
    {
      "matches": ["https://my.depauw.edu/e/reg/soc-view/results.asp"],
      "js": [
        "jquery.min.js", 
        "axios.min.js",
        "content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://my.depauw.edu/e/reg/soc-view/results.asp",
    "http://127.0.0.1:5000"
  ]
}